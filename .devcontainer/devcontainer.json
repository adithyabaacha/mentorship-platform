{
  "name": "Java Spring Boot",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "settings": {
    "terminal.integrated.defaultProfile.linux": "bash"
  },
  "extensions": [
    "vscjava.vscode-java-pack",
    "pivotal.vscode-spring-boot",
    "esbenp.prettier-vscode"
  ],
  "forwardPorts": [8080],
  "postCreateCommand": "mvn clean install",
  "remoteUser": "vscode"
}
